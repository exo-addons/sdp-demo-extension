  <properties>
    <org.exoplatform.platform.version>5.0.x-SNAPSHOT</org.exoplatform.platform.version>
    <juzu.version>1.2.0-M02</juzu.version>
    <org.data-injection-lib.version.version>1.0.0-M04</org.data-injection-lib.version.version>
  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>org.exoplatform.addons.demo</groupId>
        <artifactId>sdp-demo-webapp</artifactId>

        <version>${project.version}</version>
        <type>war</type>
      </dependency>
      <dependency>
        <groupId>org.exoplatform.platform</groupId>
        <artifactId>platform</artifactId>
        <version>${org.exoplatform.platform.version}</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
      <!-- Data Injection Lib -->
      <dependency>
        <groupId>org.exoplatform.injector</groupId>
        <artifactId>data-injection-lib</artifactId>
        <version>${org.data-injection-lib.version.version}</version>
        <type>jar</type>
      </dependency>
