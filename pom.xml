  <groupId>org.exoplatform.addons.demo</groupId>
  <artifactId>sdp-demo-extension</artifactId>
  <version>1.1.x-SNAPSHOT</version>
  <packaging>pom</packaging>
  <modules>
    <module>sdp-demo-webapp</module>
    <module>sdp-demo-packaging</module>
  </modules>
  <scm>
    <connection>scm:git:git://github.com/exo-addons/sdp-demo-extension.git</connection>
    <developerConnection>scm:git:git@github.com:exo-addons/sdp-demo-extension.git</developerConnection>
    <url>https://github.com/exo-addons/sdp-demo-extension</url>
    <tag>HEAD</tag>
