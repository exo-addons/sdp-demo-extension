<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
             version="2.0"
             xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd
   http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
    <portlet>
        <portlet-name>PopulatorApplication</portlet-name>
        <display-name xml:lang="EN">Populator Application</display-name>
        <portlet-class>org.exoplatform.addons.sdpDemo.populator.portlet.populator.PopulatorPortlet</portlet-class>
        <init-param>
            <name>juzu.run_mode</name>
            <value>prod</value>
        </init-param>
        <supports>
            <mime-type>text/html</mime-type>
        </supports>
        <portlet-info>
            <title>Populator</title>
            <short-title>Populator</short-title>
            <keywords>Content</keywords>
        </portlet-info>
    </portlet>
</portlet-app>